    "@types/prettier": "2.6.0",
    "aws-cdk": "2.45.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.45.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
