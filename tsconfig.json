{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "noUnusedLocals": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules", "cdk.out"]
}
